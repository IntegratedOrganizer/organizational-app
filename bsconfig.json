{
  "name": "rescript-template-vite",
  "bsc-flags": ["-bs-super-errors", "-bs-no-version-header"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".js",
  "bs-dependencies": [
    "@rescript/react",
    "rescript-relay",
    "@glennsl/rescript-fetch"
  ],
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "ppx-flags": ["rescript-relay/ppx"],
  "uncurried": false
}
